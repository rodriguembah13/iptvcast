{% extends 'base.html.twig' %}
{% block title %}
IPCAST|import
{% endblock %}
{% block body %}
<div class="card">
    <div class="card-header">
        <h4>Import customer</h4>
    </div>
    <div class="card-body">
        <form enctype="multipart/form-data" method="post">
            <div id="cp1" class="input-group mb-2" title="Using input value">
                <input name="file" type="file" accept="application/vnd.ms-excel" class="form-control" />
                <button type="submit" class="input-group-text colorpicker-input-addon"><i class="fa fa-upload"></i></button>
            </div>
        </form>
    </div>
</div>
    <div class="card">
        <div class="card-body">
            <table class="table table-bordered">
                <thead>
                <tr>
                    <th></th>
                    <th>Name</th>
                    <th>Phone</th>
                    <th>Card</th>
                    <th>Agence</th>
                </tr>
                </thead>
                <tbody>
                {% for cus in customers %}
                <tr>
                    <td></td>
                    <td>{{ cus.customer.compte.name }}</td>
                    <td>{{ cus.customer.compte.phone }}</td>
                    <td>{{ cus.card.numerocard }}</td>
                    <td>{{ cus.customer.agence.name }}</td>
                </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
{% endblock %}



